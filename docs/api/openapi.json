{
  "openapi": "3.0.0",
  "paths": {
    "/mcapi/data-enrich/health/ping": {
      "get": {
        "operationId": "HealthController_ping",
        "parameters": [],
        "responses": {
          "200": {
            "description": "Basic health status",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/SimpleHealthResponseDto"
                }
              }
            }
          }
        },
        "summary": "Basic health check endpoint",
        "tags": [
          "health"
        ]
      }
    },
    "/mcapi/data-enrich/health/status": {
      "get": {
        "operationId": "HealthController_status",
        "parameters": [],
        "responses": {
          "200": {
            "description": "Comprehensive health status with system metrics",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HealthResponseDto"
                }
              }
            }
          }
        },
        "summary": "Comprehensive health status endpoint",
        "tags": [
          "health"
        ]
      }
    },
    "/mcapi/data-enrich/buildings/list": {
      "get": {
        "operationId": "BuildingsController_findAll",
        "parameters": [
          {
            "name": "cadastral_code",
            "required": false,
            "in": "query",
            "description": "Cadastral code for exact matching",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "municipality_code",
            "required": false,
            "in": "query",
            "description": "Municipality code for exact matching",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "name",
            "required": false,
            "in": "query",
            "description": "Building name for case-insensitive partial matching",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "building_type",
            "required": false,
            "in": "query",
            "description": "Building type for exact matching",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "address",
            "required": false,
            "in": "query",
            "description": "Address for case-insensitive partial matching",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "polygon",
            "required": false,
            "in": "query",
            "description": "WKT polygon for spatial filtering in EPSG:4326",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": ""
          }
        },
        "security": [
          {
            "bearer": []
          }
        ],
        "summary": "Get buildings with optional filtering",
        "tags": [
          "buildings"
        ]
      }
    }
  },
  "info": {
    "title": "NestJS API",
    "description": "Production-ready NestJS API with Fastify and Pino",
    "version": "1.0.0",
    "contact": {}
  },
  "tags": [],
  "servers": [
    {
      "url": "http://localhost:3232",
      "description": "API Server"
    }
  ],
  "components": {
    "securitySchemes": {
      "bearer": {
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "type": "http"
      }
    },
    "schemas": {
      "SimpleHealthResponseDto": {
        "type": "object",
        "properties": {
          "status": {
            "description": "Basic health status",
            "type": "string"
          },
          "version": {
            "description": "Application version",
            "type": "string"
          }
        },
        "required": [
          "status",
          "version"
        ]
      },
      "HealthResponseDto": {
        "type": "object",
        "properties": {
          "status": {
            "description": "Health status of the application",
            "type": "string"
          },
          "timestamp": {
            "description": "ISO timestamp when health check was performed",
            "type": "string"
          },
          "server": {
            "description": "Server/application name",
            "type": "string"
          },
          "version": {
            "description": "Application version",
            "type": "string"
          },
          "uptime": {
            "description": "Server uptime in seconds",
            "type": "number"
          },
          "memory": {
            "description": "Memory usage statistics",
            "type": "object",
            "properties": {
              "rss": {
                "description": "Resident Set Size - total memory allocated",
                "type": "number"
              },
              "heapTotal": {
                "description": "Total heap memory allocated",
                "type": "number"
              },
              "heapUsed": {
                "description": "Heap memory currently used",
                "type": "number"
              },
              "external": {
                "description": "Memory used by C++ objects bound to JavaScript",
                "type": "number"
              },
              "arrayBuffers": {
                "description": "Memory allocated for ArrayBuffers and SharedArrayBuffers",
                "type": "number"
              }
            },
            "required": [
              "rss",
              "heapTotal",
              "heapUsed",
              "external",
              "arrayBuffers"
            ]
          },
          "environment": {
            "description": "Current environment (development, production, etc.)",
            "type": "string"
          }
        },
        "required": [
          "status",
          "timestamp",
          "server",
          "version",
          "uptime",
          "memory",
          "environment"
        ]
      }
    }
  }
}